<%= simple_form_for(@restaurant) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :adress %>
    <%= f.input :phone_number %>
    <%= f.input :caterory %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
